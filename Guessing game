import random
number=random.randint(1,9)
user=int(raw_input("pick a number in the range 1 to 9"))
if number==user:
    print "Good picking, you think like the computer"
elif user < number:
    print "sorry, you guessed  low. The computer had %d in mind" %number
    
else:
    print "sorry, you guessed  high. The computer had %d in mind" %number
