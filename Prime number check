def prime(n):
	divisors=[i for i in range(1,n+1,1) if (n%i==0)]
	if len(divisors)>2:
		print "the number is not a prime no and its divisors are %r" %divisors
	else:
		print "the number is a prime no and its only divisors are %r" %divisors


number=int(raw_input("enter a number whose primality check is required"))
prime(number)
